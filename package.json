  "private": false,
  "files": [
    "lib/**"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^8.0.16",
    "@types/tape": "^4.2.31",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.7.0",
    "typescript": "^2.6.2"
  }
}
