{
  "name": "@codificationorg/commons-core",
  "version": "1.0.0",
  "description": "A library of common utilities used in Codification.org projects.",
  "homepage": "https://github.com/CodificationOrg/commons-core",
  "bugs": "https://github.com/CodificationOrg/commons-core/issues",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "gulp clean",
    "test": "gulp prepareTest && tape -r dotenv/config -r ts-node/register \"src/**/*.spec.ts\" | tap-xunit > ./test-results/tap/results.xml",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "gulp build",
    "prepare": "yarn build",
    "prepublishOnly": "yarn preversion",
    "preversion": "yarn test && yarn lint",
    "version": "yarn format && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CodificationOrg/commons-core.git"
  },
  "keywords": [
    "common",
    "utilities",
    "support",
    "logging",
    "md5",
    "codification"
  ],
  "author": {
    "name": "William Weiss",
    "url": "https://github.com/wweiss"
  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "rxjs": "^5.5.9",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.16",
    "@types/tape": "^4.2.31",
    "del": "^3.0.0",
    "dotenv": "^5.0.1",
    "gulp": "^4.0.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "merge2": "^1.2.2",
    "prettier": "^1.14.0",
    "tap-xunit": "^2.3.0",
    "tape": "^4.8.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.6.2"
  }
}